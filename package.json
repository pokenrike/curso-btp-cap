{
  "name": "Alumnos",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/attachments": "^1.1.6",
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/textbundle": "^5.2.0",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "UI5-1.128",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-alumnos": "cds watch --open alumnos/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy Alumnos --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-cursos": "cds watch --open cursos/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-estudiantesbycursos": "cds watch --open estudiantesbycursos/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "auth": "mocked",
      "destinations": true,
      "html5-repo": true
    },
    "sql": {
      "native_hana_associations": false
    },
    "fiori": {
      "draft_deletion_timeout": "1d"
    },
    "i18n": {
      "default_language": "es",
      "folders": [
        "_i18n",
        "i18n",
        "assets/i18n"
      ]
    }
  },
  "sapux": [
    "app/alumnos",
    "app/cursos",
    "app/estudiantesbycursos"
  ]
}
